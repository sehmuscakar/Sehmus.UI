@model List<Sehmus.DataAccess.Entities.Contact>
<div class="col-lg-5 d-flex align-items-stretch">
    <div class="info">
        @foreach (var item in Model)
        {
            
        <div class="address">
            <i class="bi bi-geo-alt"></i>
            <h4>Konum:</h4>
            <p>@item.Address</p>
        </div>

        <div class="email">
            <i class="bi bi-envelope"></i>
            <h4>E-Posta:</h4>
            <p>@item.Mail</p>
        </div>

        <div class="phone">
            <i class="bi bi-phone"></i>
            <h4>Telefon:</h4>
            <p>@item.Phone</p>
        </div>
        }

    </div>

</div>