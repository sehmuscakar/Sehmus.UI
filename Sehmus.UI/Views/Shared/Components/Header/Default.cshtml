@model List<Sehmus.DataAccess.Entities.Header>
<header id="header">
    <div class="d-flex flex-column">
        @foreach (var item in Model)
        {

        <div class="profile">
           
           
            <img src="~/Img/@item.ImageUrl" alt="" class="img-fluid rounded-circle">
            <h1 class="text-light"><a href="index.html">@item.FullName</a></h1>
            <div class="social-links mt-3 text-center">
                    <a href="@item.Youtube" class="twitter"><i class="bx bxl-youtube"></i></a>
                    <a href="@item.İnstagram" class="instagram"><i class="bx bxl-instagram"></i></a>
                    <a href="@item.Github" class="google-plus"><i class="bx bxl-github"></i></a>
                    <a href="@item.Linkedin" class="linkedin"><i class="bx bxl-linkedin"></i></a>
            </div>
        </div>

        <nav id="navbar" class="nav-menu navbar">
            <ul>
                <li><a href="/Home/Anasayfa" class="nav-link scrollto active"><i class="bx bx-home"></i> <span>Anasayfa</span></a></li>
                <li><a href="#about" class="nav-link scrollto"><i class="bx bx-user"></i> <span>Hakkımda</span></a></li>
                <li><a href="#resume" class="nav-link scrollto"><i class="bx bx-file-blank"></i> <span>Özgeçmiş</span></a></li>
                <li><a href="#portfolio" class="nav-link scrollto"><i class="bx bx-book-content"></i> <span>Portföy</span></a></li>
                <li><a href="#contact" class="nav-link scrollto"><i class="bx bx-envelope"></i> <span>İletişim</span></a></li>
            </ul>
        </nav><!-- .nav-menu -->
        }
    </div>
</header><!-- End Header -->
            