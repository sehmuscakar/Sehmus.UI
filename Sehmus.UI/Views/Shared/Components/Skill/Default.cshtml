@model List<Sehmus.DataAccess.Entities.Skill>
<section id="skills" class="skills section-bg">
    <div class="container">

        <div class="section-title">
            <h2>Yetenekler</h2>
        </div>
        @foreach (var item in Model)
        {
            
        
        <div class="row skills-content">

            <div class="col-lg-6" data-aos="fade-up">

                <div class="progress">
                    <span class="skill">@item.Title <i class="val">@item.Number%</i></span>
                    <div class="progress-bar-wrap">
                            <div class="progress-bar" role="progressbar" aria-valuenow="@item.Number" aria-valuemin="0" aria-valuemax="100"></div>
                    </div>
                </div>  

            </div>

        </div>
        }
    </div>
</section><!-- End Skills Section -->
