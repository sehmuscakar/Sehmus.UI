    <link href="~/iPortfolio/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/iPortfolio/assets/css/style.css" rel="stylesheet">

    <!-- =======================================================
