</head>

<body>

    <!-- ======= Mobile nav toggle button ======= -->
    <i class="bi bi-list mobile-nav-toggle d-xl-none"></i>

    <!-- ======= Header ======= -->
@await Component.InvokeAsync("Header")
@await Component.InvokeAsync("Hero")

 

    <main id="main">

        <!-- ======= About Section ======= -->
        @await Component.InvokeAsync("About")
        <!-- ======= Skills Section ======= -->
        @await Component.InvokeAsync("Skill")

        <!-- ======= Resume Section ======= -->
        @await Component.InvokeAsync("Resume")
        <!-- ======= Portfolio Section ======= -->
        @await Component.InvokeAsync("Portfolio")
        <!-- ======= Contact Section ======= -->
        <section id="contact" class="contact">
            <div class="container">

                <div class="section-title">
                    <h2>İletişim</h2>
                </div>

                <div class="row" data-aos="fade-in">

                   @await Component.InvokeAsync("Contact")

                    <div class="col-lg-7 mt-5 mt-lg-0 d-flex align-items-stretch">
                        <form action="forms/contact.php" method="post" role="form" class="php-email-form">
                            <div class="row">
                                <div class="form-group col-md-6">
                                    <label for="name">Adınız</label>
                                    <input type="text" name="name" class="form-control" id="name" required>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="name">Mailiniz</label>
                                    <input type="email" class="form-control" name="email" id="email" required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="name">Konu</label>
                                <input type="text" class="form-control" name="subject" id="subject" required>
                            </div>
                            <div class="form-group">
                                <label for="name">Mesaj</label>
                                <textarea class="form-control" name="message" rows="10" required></textarea>
                            </div>
                            <div class="my-3">
                                <div class="loading">Loading</div>
                                <div class="error-message"></div>
                                <div class="sent-message">Your message has been sent. Thank you!</div>
                            </div>
                            <div class="text-center"><button type="submit">Gönder</button></div>
                        </form>
                    </div>

                </div>

            </div>
        </section><!-- End Contact Section -->

    </main><!-- End #main -->

    <div class="container">
    <main role="main" class="pb-3">
            @RenderBody()
            </main>
       </div>

    <!-- ======= Footer ======= -->
    <footer id="footer">
        <div class="container">
            <div class="copyright">
                <p class="copyright">Copyright &copy; <span id="current-year"></span> <strong>Ali Asaf Günşar</strong> Tüm Hakları Saklıdır.</p>
            </div>
          
        </div>
    </footer><!-- End  Footer -->
       <script>
        document.getElementById("current-year").innerHTML = new Date().getFullYear();
 </script>

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/iPortfolio/assets/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/iPortfolio/assets/vendor/aos/aos.js"></script>
    <script src="~/iPortfolio/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/iPortfolio/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/iPortfolio/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/iPortfolio/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/iPortfolio/assets/vendor/typed.js/typed.umd.js"></script>
    <script src="~/iPortfolio/assets/vendor/waypoints/noframework.waypoints.js"></script>
    <script src="~/iPortfolio/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/iPortfolio/assets/js/main.js"></script>

</body>

</html>