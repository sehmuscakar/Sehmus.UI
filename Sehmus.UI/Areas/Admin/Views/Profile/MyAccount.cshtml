@model Sehmus.UI.Areas.Admin.Models.AppUserEditModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>Ali Asaf Günşar Hesap Paneli</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/Img/admintitle.jpg" rel="icon" sizes="32x32">
    <link href="~/Img/admintitle.jpg" rel="apple-touch-icon" sizes="180x180">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/AdminTema/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/AdminTema/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/AdminTema/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/AdminTema/assets/vendor/quill/quill.snow.css" rel="stylesheet">
    <link href="~/AdminTema/assets/vendor/quill/quill.bubble.css" rel="stylesheet">
    <link href="~/AdminTema/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/AdminTema/assets/vendor/simple-datatables/style.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/AdminTema/assets/css/style.css" rel="stylesheet">
</head>
<body>
    <main id="main" class="main">
        <div class="pagetitle">
            <h1>Hesap Bilgileri</h1>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="index.html">Yönetim Paneli</a></li>
                    <li class="breadcrumb-item">Profil Bilgileri</li>
                </ol>
            </nav>
        </div><!-- End Page Title -->
        <section class="section profile">
            <div class="col-xl-8">
                <!-- Profile Edit Form -->
                <form method="post" enctype="multipart/form-data">
                    <div class="row mb-3">
                        <label asp-for="ImageUrl" class="col-md-4 col-lg-3 col-form-label">Resminiz</label>
                        <div class="col-md-8 col-lg-9">
                            <input asp-for="ImageUrl" type="file" class="form-control">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <label asp-for="Name" class="col-md-4 col-lg-3 col-form-label">Adınız</label>
                        <div class="col-md-8 col-lg-9">
                            <input asp-for="Name" type="text" class="form-control">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <label asp-for="LastName" class="col-md-4 col-lg-3 col-form-label">Soyadınız</label>
                        <div class="col-md-8 col-lg-9">
                            <input asp-for="LastName" type="text" class="form-control">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <label asp-for="About" class="col-md-4 col-lg-3 col-form-label">Hakkınızda</label>
                        <div class="col-md-8 col-lg-9">
                            <textarea asp-for="About" class="form-control" style="height: 100px"></textarea>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <label asp-for="Adress" class="col-md-4 col-lg-3 col-form-label">Addres</label>
                        <div class="col-md-8 col-lg-9">
                            <input asp-for="Adress" type="text" class="form-control" id="Address">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <label asp-for="Phone" class="col-md-4 col-lg-3 col-form-label">Telefon</label>
                        <div class="col-md-8 col-lg-9">
                            <input asp-for="Phone" type="text" class="form-control">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <label asp-for="Email" class="col-md-4 col-lg-3 col-form-label">Mail</label>
                        <div class="col-md-8 col-lg-9">
                            <input asp-for="Email" type="email" class="form-control">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <label asp-for="Password" class="col-md-4 col-lg-3 col-form-label">Yeni Şifre</label>
                        <div class="col-md-8 col-lg-9">
                            <input asp-for="Password" type="password" class="form-control">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <label asp-for="ConfirmPassword" class="col-md-4 col-lg-3 col-form-label">Tekrar Şifre Girin</label>
                        <div class="col-md-8 col-lg-9">
                            <input asp-for="ConfirmPassword" type="password" class="form-control">
                        </div>
                    </div>
                    <div class="text-center">
                        <button type="submit" class="btn btn-primary">Değişiklikleri Kaydet</button>
                    </div>
                </form><!-- End Profile Edit Form -->
            </div>
        </section>
    </main><!-- End #main -->
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/AdminTema/assets/vendor/apexcharts/apexcharts.min.js"></script>
    <script src="~/AdminTema/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/AdminTema/assets/vendor/chart.js/chart.umd.js"></script>
    <script src="~/AdminTema/assets/vendor/echarts/echarts.min.js"></script>
    <script src="~/AdminTema/assets/vendor/quill/quill.min.js"></script>
    <script src="~/AdminTema/assets/vendor/simple-datatables/simple-datatables.js"></script>
    <script src="~/AdminTema/assets/vendor/tinymce/tinymce.min.js"></script>
    <script src="~/AdminTema/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/AdminTema/assets/js/main.js"></script>
</body>

</html>